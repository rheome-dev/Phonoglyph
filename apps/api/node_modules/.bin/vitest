#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@20.19.1_jsdom@26.1.0_msw@2.2.1_typescript@5.3.3__terser@5.43.1/node_modules/vitest/node_modules:/workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@20.19.1_jsdom@26.1.0_msw@2.2.1_typescript@5.3.3__terser@5.43.1/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@20.19.1_jsdom@26.1.0_msw@2.2.1_typescript@5.3.3__terser@5.43.1/node_modules/vitest/node_modules:/workspace/node_modules/.pnpm/vitest@3.2.4_@types+node@20.19.1_jsdom@26.1.0_msw@2.2.1_typescript@5.3.3__terser@5.43.1/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
